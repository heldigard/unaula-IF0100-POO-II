      "source": [
        "# \ud83d\dd2e ESCRIBE TU C\u0000d3DIGO AQU\u0000cd\n",
        "\n",
        "# 1. Pedir el primer n\u0000famero (convertir a float)\n",
        "num1 = None  # \ud83d\udd12 Reemplazar con input()\n",
        "\n",
        "# 2. Pedir el segundo n\u0000famero (convertir a float)\n",
        "num2 = None  # \ud83d\udd12 Reemplazar con input()\n",
        "\n",
        "# 3. Pedir la operaci\u0003n (+, -, *, /)\n",
        "operacion = None  # \ud83d\udd12 Reemplazar con input()\n",
        "\n",
        "# 4. Realizar la operaci\u0003n seg\u0000fan el operador\n",
        "resultado = None  # \ud83d\udd12 Completar la l\u0000f3gica\n",
