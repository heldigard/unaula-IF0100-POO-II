  {"cell_type":"code","execution_count":null,"id":"cell-17","metadata":{},"outputs":[],"source":["# ðŸ”µ REFACTOR: Usar next() para bÃºsqueda mÃ¡s pythonica\n","\nclass UsuarioRepository:\n","    \"\"\"Repositorio de usuarios en memoria para TaskFlow.\"\"\"\n","    \n","    def __init__(self) -> None:\n","        self._usuarios: List[Usuario] = []\n","        self._siguiente_id: int = 1\n","    \n","    def guardar(self, usuario: Usuario) -> None:\n","        if usuario.id is None:\n","            usuario.id = self._siguiente_id\n","            self._siguiente_id += 1\n","        self._usuarios.append(usuario)\n","    \n","    def buscar_por_id(self, usuario_id: int) -> Optional[Usuario]:\n","        \"\"\"\n","        Busca un usuario por su ID.\n","        \n","        Returns:\n","            El usuario encontrado o None\n","        \"\"\"\n","        return next(\n","            (u for u in self._usuarios if u.id == usuario_id),\n","            None\n","        )\n","\n","# Verificar refactor\n","test_buscar_por_id_retorna_usuario()\n","print(\"âœ… Test sigue pasando despuÃ©s del refactor\")"]},
