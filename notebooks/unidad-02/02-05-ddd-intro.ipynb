  {"cell_type":"code","execution_count":null,"id":"cell-18","metadata":{},"outputs":[],"source":["# SoluciÃ³n completa\n","@dataclass(frozen=True)\nclass FechaLimite:\n","    \"\"\"Value Object: Fecha lÃ­mite de una tarea.\"\"\"\n","    valor: date\n","    \n","    def __post_init__(self):\n","        \"\"\"Valida la fecha lÃ­mite.\"\"\"\n","        hoy = date.today()\n","        \n","        # No puede ser fecha pasada\n","        if self.valor < hoy:\n","            raise ValueError(\"La fecha lÃ­mite no puede ser en el pasado\")\n","        \n","        # No puede ser mÃ¡s de 1 aÃ±o en futuro\n","        max_futuro = hoy + timedelta(days=365)\n","        if self.valor > max_futuro:\n","            raise ValueError(\"La fecha lÃ­mite no puede ser mÃ¡s de 1 aÃ±o en el futuro\")\n","    \n","    def dias_restantes(self) -> int:\n","        \"\"\"Retorna los dÃ­as restantes hasta la fecha lÃ­mite.\"\"\"\n","        return (self.valor - date.today()).days\n","    \n","    def es_vencida(self) -> bool:\n","        \"\"\"Verifica si la fecha estÃ¡ vencida.\"\"\"\n","        return self.valor < date.today()\n","    \n","    def __str__(self) -> str:\n","        return self.valor.strftime(\"%Y-%m-%d\")\n","\n","# Probar FechaLimite\n","try:\n","    # Fecha vÃ¡lida\n","    fecha_valida = FechaLimite(date.today() + timedelta(days=7))\n","    print(f\"Fecha vÃ¡lida: {fecha_valida}\")\n","    print(f\"DÃ­as restantes: {fecha_valida.dias_restantes()}\")\n","    print(f\"Â¿Vencida? {fecha_valida.es_vencida()}\")\n","    \n","    # Fecha invÃ¡lida (pasado)\n","    # fecha_invalida = FechaLimite(date.today() - timedelta(days=1))\n","    \n","except ValueError as e:\n","    print(f\"\\nâœ… ValidaciÃ³n funcionÃ³: {e}\")"]},
