  {"cell_type":"code","execution_count":null,"id":"cell-8","metadata":{},"outputs":[],"source":["from enum import Enum\n","\n","class Prioridad(Enum):\n","    \"\"\"\n","    Value Object: Prioridad de una tarea.\n","    \n","    Usamos Enum porque:\n","    - Valores finitos y predefinidos\n","    - Tipo fuerte (no es string)\n","    - Inmutable por naturaleza\n","    \"\"\"\n","    BAJA = \"baja\"\n","    MEDIA = \"media\"\n","    ALTA = \"alta\"\n","    URGENTE = \"urgente\"\n","    \n","    def peso(self) -> int:\n","        \"\"\"Retorna el peso numÃ©rico para comparaciones.\"\"\"\n","        pesos = {\n","            self.BAJA: 1,\n","            self.MEDIA: 2,\n","            self.ALTA: 3,\n","            self.URGENTE: 4\n","        }\n","        return pesos[self]\n","    \n","    def es_mayor_que(self, otra: 'Prioridad') -> bool:\n","        \"\"\"Verifica si esta prioridad es mayor que otra.\"\"\"\n","        return self.peso() > otra.peso()\n","    \n","    def __str__(self) -> str:\n","        return self.value\n","\n","# Probar Prioridad\n","prioridad1 = Prioridad.ALTA\n","prioridad2 = Prioridad.MEDIA\n","prioridad3 = Prioridad.ALTA\n","\n","print(f\"Prioridad 1: {prioridad1}\")\n","print(f\"Prioridad 2: {prioridad2}\")\n","print(f\"Â¿Prioridad 1 > Prioridad 2? {prioridad1.es_mayor_que(prioridad2)}\")\n","print(f\"Â¿Prioridad 1 == Prioridad 3? {prioridad1 == prioridad3}\")"]},
