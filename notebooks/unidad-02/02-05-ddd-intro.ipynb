  {"cell_type":"code","execution_count":null,"id":"cell-15","metadata":{},"outputs":[],"source":["from abc import ABC, abstractmethod\n","from typing import TypeVar, Generic\n","\n","T = TypeVar('T')  # Tipo genÃ©rico\n","\n","class Repository(ABC, Generic[T]):\n","    \"\"\"\n","    Interface base para repositorios.\n","    \n","    NOTA: Solo repositorios de AGGREGATE ROOTS.\n","    No hay repositorio de Tarea, solo de Proyecto.\n","    \"\"\"\n","    \n","    @abstractmethod\n","    def guardar(self, entidad: T) -> None:\n","        \"\"\"Guarda una entidad (crear o actualizar).\"\"\"\n","        pass\n","    \n","    @abstractmethod\n","    def buscar_por_id(self, entidad_id: int) -> Optional[T]:\n","        \"\"\"Busca una entidad por su ID.\"\"\"\n","        pass\n","    \n","    @abstractmethod\n","    def listar_todos(self) -> List[T]:\n","        \"\"\"Retorna todas las entidades.\"\"\"\n","        pass\n","    \n","    @abstractmethod\n","    def eliminar(self, entidad_id: int) -> bool:\n","        \"\"\"Elimina una entidad por su ID.\"\"\"\n","        pass\n","\n","class ProyectoRepository(Repository[Proyecto]):\n","    \"\"\"\n","    Repositorio de proyectos (Aggregate Root).\n","    \n","    NOTA: No existe TareaRepository. Las tareas solo\n","    se acceden a travÃ©s del Proyecto agregado.\n","    \"\"\"\n","    \n","    def __init__(self):\n","        self._proyectos: dict[int, Proyecto] = {}\n","        self._next_id = 1\n","    \n","    def guardar(self, proyecto: Proyecto) -> None:\n","        \"\"\"Guarda un proyecto (crear o actualizar).\"\"\"\n","        if proyecto.id is None:\n","            proyecto.id = self._next_id\n","            self._next_id += 1\n","        \n","        self._proyectos[proyecto.id] = proyecto\n","    \n","    def buscar_por_id(self, proyecto_id: int) -> Optional[Proyecto]:\n","        \"\"\"Busca un proyecto por ID.\"\"\"\n","        return self._proyectos.get(proyecto_id)\n","    \n","    def listar_todos(self) -> List[Proyecto]:\n","        \"\"\"Retorna todos los proyectos.\"\"\"\n","        return list(self._proyectos.values())\n","    \n","    def eliminar(self, proyecto_id: int) -> bool:\n","        \"\"\"Elimina un proyecto por ID.\"\"\"\n","        if proyecto_id in self._proyectos:\n","            del self._proyectos[proyecto_id]\n","            return True\n","        return False\n","    \n","    def listar_por_propietario(self, propietario_id: int) -> List[Proyecto]:\n","        \"\"\"\n","        MÃ©todo especÃ­fico del dominio.\n","        \n","        Los repositorios pueden tener mÃ©todos especÃ­ficos\n","        ademÃ¡s de los genÃ©ricos.\n","        \"\"\"\n","        return [\n","            p for p in self._proyectos.values()\n","            if p.propietario_id == propietario_id\n","        ]\n","\n","# Probar el repositorio\n","repo = ProyectoRepository()\n","proyecto = Proyecto(\n","    id=None,\n","    nombre=\"TaskFlow\",\n","    descripcion=\"Sistema\",\n","    propietario_id=1\n",")\n","\n","repo.guardar(proyecto)\n","print(f\"Proyecto guardado con ID: {proyecto.id}\")\n","\n","encontrado = repo.buscar_por_id(proyecto.id)\n","print(f\"Proyecto encontrado: {encontrado.nombre if encontrado else 'None'}\")"]},
