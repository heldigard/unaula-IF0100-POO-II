  {"cell_type":"code","execution_count":null,"id":"cell-7","metadata":{},"outputs":[],"source":["@dataclass(frozen=True)\n","class Email:\n","    \"\"\"\n","    Value Object: Email.\n","    \n","    Un email es un Value Object porque:\n","    - No tiene identidad propia (se compara por valor)\n","    - Es inmutable (frozen=True)\n","    - Tiene lÃ³gica de validaciÃ³n propia\n","    \"\"\"\n","    valor: str\n","    \n","    def __post_init__(self):\n","        \"\"\"Valida el email despuÃ©s de la creaciÃ³n.\"\"\"\n","        if not self._es_valido():\n","            raise ValueError(f\"Email '{self.valor}' es invÃ¡lido\")\n","    \n","    def _es_valido(self) -> bool:\n","        \"\"\"Verifica que el email tenga formato vÃ¡lido.\"\"\"\n","        if \"@\" not in self.valor:\n","            return False\n","        \n","        partes = self.valor.split(\"@\")\n","        if len(partes) != 2:\n","            return False\n","        \n","        usuario, dominio = partes\n","        if not usuario or not dominio:\n","            return False\n","        \n","        if \".\" not in dominio:\n","            return False\n","        \n","        return True\n","    \n","    def dominio(self) -> str:\n","        \"\"\"Retorna el dominio del email.\"\"\"\n","        return self.valor.split(\"@\")[1]\n","    \n","    def __str__(self) -> str:\n","        return self.valor\n","\n","# Probar el Value Object Email\n","try:\n","    email1 = Email(\"john@example.com\")\n","    email2 = Email(\"john@example.com\")\n","    email3 = Email(\"alice@example.com\")\n","    \n","    print(f\"Email 1: {email1}\")\n","    print(f\"Email 2: {email2}\")\n","    print(f\"Â¿Email 1 == Email 2? {email1 == email2}\")\n","    print(f\"Â¿Email 1 == Email 3? {email1 == email3}\")\n","    print(f\"Dominio del email 1: {email1.dominio()}\")\n","    \n","    # Intentar crear email invÃ¡lido\n","    email_invalido = Email(\"invalido\")\n","except ValueError as e:\n","    print(f\"\\nâœ… ValidaciÃ³n funcionÃ³: {e}\")"]},
